/* pages/user/dingdan.wxss */
@import "../../wxParse/wxParse.wxss";

/*供求*/

.scr {
  white-space: nowrap;
  height: 103rpx;
  position: fixed;
  top: 0;
}

.tab {
  flex-direction: row;
  width: 100%;
}

.tab-left {
  width: 100%;
  font-size: 30rpx;
  background: #fff;
}

.tab-left .ah {
  text-align: center;
  display: inline-block;
  padding: 15rpx 30rpx;
  padding-bottom: 0rpx;
}

.tab-left .active {
  color: #f00;
  border-bottom: solid 1px red;
}

.tab-right {
  margin-top:104rpx;
}
.tab-right .right-item{
  display: none;
}
.tab-right .right-item.active {
  display: block;
}
